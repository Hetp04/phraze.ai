{
  "manifest_version": 3,
  "name": "Label Adder",
  "description": "A brief description of your extension",
  "version": "1.1",
  "key": "",
  "width": 300,
  "height": 600,
  "permissions": [
    "identity",
    "contextMenus",
    "activeTab",
    "tabs",
    "scripting",
    "storage",
    "declarativeContent",
    "desktopCapture",
    "downloads",
    "https://*.firebaseio.com/*",
    "https://*.firebaseapp.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "popper.min.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "highlights.html",
    "default_icon": "img/favicon-32x32.png",
    "default_title": "Note App"
  },
  "icons": {
    "192": "img/dashboard-android-chrome-192x192.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "lib/*",
        "lib/chart.js",
        "img/data-preview-arrow.png"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; connect-src 'self' https://api.groq.com/ https://*.firebaseio.com wss://*.firebaseio.com https://*.firebaseapp.com https://*.googleapis.com https://identitytoolkit.googleapis.com https://people.googleapis.com"
  }
}